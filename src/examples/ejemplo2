&& Este es un comentario de una linea.
numero comienzo() {
	booleano x = verdadero y falso o 84 <= 0
	si(!x) {
		numero nombreVariable = 0
		hacer {
			imprimirln(nombreVariable)
			nombreVariable = nombreVariable+2
		} mientras(nombreVariable <= 20)
	}
	sino {
		repetir(10) {
			imprimirln("La expresion era verdadera")
		}
	}
	devolver 0
}
